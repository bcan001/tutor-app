<h1>Sign Up for CourseDiscuss</h1>

<%= form_for @user do |f| %>

  <p>First Name</p>
  <p><%= f.text_field :first_name %></p>

  <p>Last Name</p>
  <p><%= f.text_field :last_name %></p>

  <p>Email</p>
  <p><%= f.text_field :email %></p>

  <p>Username</p>
  <p><%= f.text_field :username %></p>
  <p>Password</p>
  <p><%= f.password_field :password %></p>

  <p><%= f.submit 'Sign Up', class: "btn btn-primary" %></p>

<% end %>